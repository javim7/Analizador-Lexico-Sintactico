	graph [rankdir=LR]
	0 -> 1 [label=a]
	0 -> 0 [label=b]
	1 -> 1 [label=a]
	1 -> 2 [label=b]
	2 -> 1 [label=a]
	2 -> 3 [label=b]
	3 -> 1 [label=a]
	3 -> 0 [label=b]
