/* Gramatica SLR: */
/* E -> E + T | T */
/* T -> t * F | F */
/* F -> ( E ) | id */

%token ID
%token PLUS
%token TIMES
%token LPAREN
%token RPAREN
%token WS
IGNORE WS

%%

expression:
    expression PLUS term
    | term
;
term:
    term TIMES factor
    | factor
;
factor:
    LPAREN expression RPAREN
    | ID
;
