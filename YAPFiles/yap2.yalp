/* Nueva Gramática SLR: */
/* S -> E */
/* E -> T E2 */
/* E2 -> + T E2 | ε */
/* T -> F T2 */
/* T2 -> * F T2 | ε */
/* F -> ( E ) | id */

%token ID
%token PLUS
%token TIMES
%token LPAREN RPAREN
%token WS
IGNORE WS

%%
start:
    expression
;
expression:
    term expression2
;
expression2:
    PLUS term expression2
    |
;
term:
    factor term2
;
term2:
    TIMES factor term2
    |
;
factor:
    LPAREN expression RPAREN
    | ID
;
