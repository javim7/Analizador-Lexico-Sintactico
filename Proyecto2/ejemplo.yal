(*ejemplo.yal*)
{
import myToken
}
rule gettoken = 
    [' ''\t''\n'] { return lexbuf } (* skip blanks *)
    | ['0'-'9']+ { return int(lxm) }
    | '+' { return PLUS }
    | '-' { return MINUS }
    | '*' { return TIMES }
    | '/' { return DIV }
    | '(' { return LPAREN }
    | ')' { return RPAREN }
    | eof { raise( 'Fin de buffer' )}