(*ejemplo.yal*)
{
import myToken
}
rule gettoken = 
    [' ''\t''\n'] { print("lexbuf")  } (* skip blanks *)
    | ['0'-'9']+ { print("int") }
    | '+' { print("PLUS")  }
    | '-' { print("MINUS")  }
    | '*' { print("TIMES")  }
    | '/' { print("DIV")  }
    | '(' { print("LPAREN")  }
    | ')' { print("RPAREN")  }
    | eof { raise( 'Fin de buffer' )}